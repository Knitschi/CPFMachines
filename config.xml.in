<?xml version='1.0' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.12.1">
  <actions>
    <io.jenkins.blueocean.service.embedded.BlueOceanUrlAction plugin="blueocean-rest-impl@1.1.5">
      <blueOceanUrlObject class="io.jenkins.blueocean.service.embedded.BlueOceanUrlObjectImpl">
        <mappedUrl>blue/organizations/jenkins/@JOB_NAME@</mappedUrl>
      </blueOceanUrlObject>
    </io.jenkins.blueocean.service.embedded.BlueOceanUrlAction>
  </actions>
  <description>A CMakeProjectFramework build-job for repository: @CI_REPOSITORY@.&#xd;
&#xd;
Note that this job is generated by the CPFJenkinsjob package and manual changes to it will not be permanent! &#xd;
&#xd;
A documentation of the jobs parameters can be found at: http://buildmasterdebian9/doxygen/df/d9d/group___c_p_f_jenkinsjob_group.html &#xd;
&#xd;
</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>-1</daysToKeep>
        <numToKeep>5</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>taggingOption</name>
          <description>Determines if and how a successful build should be tagged. 
Possible values are: internal, incrementMajor &lt;package&gt;, incrementMinor &lt;package&gt;, incrementPatch &lt;package&gt;
</description>
          <defaultValue>internal</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>branchOrTag</name>
          <description>Determines the git reference that will be build. This can be a branch, tag or a commit hash.
Examples:
Branch: master
Tag: 0.1.3
Commit ID: 95e0a6
</description>
          <defaultValue>master</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>cpfConfiguration</name>
          <description>This parameter can be used to only build one configuration.
If no value is set, the job will build all configurations that are listed in the
Sources/BuildConfigurations/TestedConfigurations.txt
file.
</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>target</name>
          <description>The CMakeProjectFramework target that is build.
This parameter is intended for reducing build times during debugging.
</description>
          <defaultValue>pipeline</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>cpfCIRepository</name>
          <description>The address of the CPF project repository that is build by this job.
This parameter is an implementation detail and is not supposed to be changed by the user.
</description>
          <defaultValue>@CI_REPOSITORY@</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>buildResultRepositoryMaster</name>
          <description>
The address of the git repository to which the installed files of the
build project are committed.
This parameter is an implementation detail and is not supposed to be changed by the user.
          </description>
          <defaultValue>@BUILD_RESULT_REPOSITORY_MASTER@</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>buildResultRepositoryWebServer</name>
          <description>
An optional parameter that holds the address to a clone of buildResultRepositoryMaster to which
the created build results are additionally pushed.
This parameter is an implementation detail and is not supposed to be changed by the user.
          </description>
          <defaultValue>@BUILD_RESULT_REPOSITORY_WEB_SERVER@</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>buildResultRepositorySubdirectory</name>
          <description>
The subdirectory in the build result repository to which the installed files are added.
This parameter is an implementation detail and is not supposed to be changed by the user.
          </description>
          <defaultValue>@BUILD_RESULT_REPOSITORY_SUBDIRECTORY@</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>CPFCMake_DIR</name>
          <description>
The path to CPFCMake. This is required when running the 0_CopyScripts script.
          </description>
          <defaultValue>@CPFCMake_DIR@</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>CPFBuildscripts_DIR</name>
          <description>
The path to CPFBuildscripts. This is required when running the 0_CopyScripts script.
          </description>
          <defaultValue>@CPFBuildscripts_DIR@</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>CIBuildConfigurations_DIR</name>
          <description>
The path to the CIBuildConfigurations directory. This is required when running the 0_CopyScripts script.
          </description>
          <defaultValue>@CIBuildConfigurations_DIR@</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
    <org.jenkinsci.plugins.workflow.job.properties.DisableConcurrentBuildsJobProperty/>
    <org.jenkinsci.plugins.workflow.job.properties.DurabilityHintJobProperty>
      <hint>PERFORMANCE_OPTIMIZED</hint>
    </org.jenkinsci.plugins.workflow.job.properties.DurabilityHintJobProperty>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.37">
    <scm class="hudson.plugins.git.GitSCM" plugin="git@3.5.0">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>@CPFMACHINES_REPOSITORY@</url>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>@JENKINSFILE_TAG_OR_BRANCH@</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
    <scriptPath>jenkinsfile</scriptPath>
    <lightweight>false</lightweight>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>